{
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.dart": "$(capture).*.dart",
    "*.js": "$(capture).css, $(capture).*.js",
    "*.ts": "$(capture).css, $(capture).*.ts",
    "firebase.json": ".firebaserc",
    "pubspec.yaml": "*.lock, *.yaml, *.yml, .metadata, .flutter-plugins, .flutter-plugins-dependencies, example.iml",
    "package.json": "*.json, .nvmrc, .eslintrc.js, .prettierrc, .firebaserc",
    ".gitignore": ".*ignore",
    "README.md": "*.md"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "[yaml]": {
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "files.exclude": {
    "**/.dart_tool": true,
    "**/.flutter-plugins**": true,
    "**/.metadata": true,
    "**/pubspec.lock": true,
    "**/node_modules/": true,
    "**.firebase": true,
  },
  "dart.flutterCreateOrganization": "co.mankeli",
  "dart.flutterGenerateLocalizationsOnSave": "all",
  "dart.runPubGetOnNestedProjects": "both"
}
